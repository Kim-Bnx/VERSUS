{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript][typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.showKeywords": false
  },
  "editor.formatOnSave": true,
  "editor.detectIndentation": true,
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "files.exclude": {
    "USE_GITIGNORE": true
  },
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "markdown"
  ],
  "javascript.validate.enable": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "markdown": "html",
    "ejs": "html"
  },
  "workbench.iconTheme": "material-icon-theme",
  "cSpell.words": [
    "airbnb"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fc8ba4",
    "activityBar.background": "#fc8ba4",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#e7fee0",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "editorGroup.border": "#fc8ba4",
    "sash.hoverBorder": "#fc8ba4",
    "sideBar.border": "#fc8ba4",
    "statusBar.background": "#fb597d",
    "statusBar.debuggingBackground": "#59fbd7",
    "statusBar.debuggingForeground": "#15202b",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#fa2756",
    "statusBarItem.remoteBackground": "#fb597d",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#fb597d",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#fb597d99",
    "titleBar.inactiveForeground": "#15202b99"
  }
}
